meta {
  name: VaultSecret
  type: http
  seq: 4
}

post {
  url: {{VAULTURL}}/v1/secret/data/consumersecret
  body: json
  auth: inherit
}

headers {
  X-Vault-Token: vaultsecret0123456789
}

body:json {
  {
    "data": {
      "content": "{{CONSUMER_STS_SECRET}}"
    }
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
